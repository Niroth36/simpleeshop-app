{
  "name": "simpleeshop",
  "version": "1.0.0",
  "description": "A simple e-commerce application with Node.js and PostgreSQL",
  "main": "server/server_postgresql.js",
  "scripts": {
    "start": "node server/server_postgresql.js",
    "dev": "nodemon server/server_postgresql.js",
    "docker:build": "docker build -t simpleeshop .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["ecommerce", "nodejs", "postgresql", "express"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "body-parser": "^1.20.2",
    "express-session": "^1.17.3",
    "cors": "^2.8.5",
    "redis": "^4.6.12",
    "connect-redis": "^7.1.0",
    "minio": "^7.1.3",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
